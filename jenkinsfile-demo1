    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build triggered using Jenkinsfile'
                sh 'ls -la'
                sh 'date'
                echo 'Build completed successfully'
                echo 'Yaaaga'
                echo 'GitHub webhook integration successful!'
                echo 'Automated build process initiated!...'
            }
        }
    }
}
